@model AnnouncementUpdateModel
@{
    ViewBag.Title = "Edit Announcement";
}

<h2>@ViewBag.Title</h2>

<form asp-action="Edit" method="post">
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.Id)

    @Html.Partial("_AnnouncementForm", Model)

    <button type="submit" id="submitAnnouncementFormBtn" class="btn btn-primary">Save Changes</button>
    <a asp-action="Index" class="btn btn-secondary">Back to list</a>
</form>
